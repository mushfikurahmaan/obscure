{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Obscure",
  "version": "0.1.0",
  "identifier": "com.obscure.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Obscure",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "minHeight": 626,
        "minWidth": 612,
        "additionalBrowserArgs": "--disable-features=PasswordManager,AutofillPasswordGeneration"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}